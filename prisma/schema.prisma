generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Feedback {
  id              Int      @id @default(autoincrement())
  name            String?
  serviceInterest Boolean
  newServiceIdea  String
  createdAt       DateTime @default(now())
}
